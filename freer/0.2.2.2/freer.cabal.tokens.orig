    extra-source-files:
        README.md
        changelog.md
        CODE_OF_CONDUCT.md

    source-repository head
        type: git
        location: git clone https://gitlab.com/cpp.cabrera/freer.git

    library
      exposed-modules:     Control.Monad.Freer
                         , Control.Monad.Freer.Coroutine
                         , Control.Monad.Freer.Cut
                         , Control.Monad.Freer.Exception
                         , Control.Monad.Freer.Fresh
                         , Control.Monad.Freer.Internal
                         , Control.Monad.Freer.Reader
                         , Control.Monad.Freer.State
                         , Control.Monad.Freer.StateRW
                         , Control.Monad.Freer.Trace
                         , Control.Monad.Freer.Writer
                         , Data.FTCQueue
                         , Data.Open.Union

      build-depends:       base >=4.7 && <4.9
      hs-source-dirs:      src
      ghc-options:         -Wall
      default-language:    Haskell2010

    executable examples
      main-is:             Main.hs
      other-modules:       Common
                         , Coroutine
                         , Cut
                         , Fresh
                         , NonDetEff
                         , Teletype
                         , Trace
      build-depends:       base >=4.7 && <4.9
                         , freer
      hs-source-dirs:      examples/src
      ghc-options:         -Wall
      default-language:    Haskell2010

    test-suite test
      default-language: Haskell2010
      type:             exitcode-stdio-1.0
      hs-source-dirs:   tests
      main-is:          Tests.hs
      other-modules:       Tests.Common
                         , Tests.Exception
                         , Tests.Fresh
                         , Tests.NonDetEff
                         , Tests.Reader
                         , Tests.State
                         , Tests.StateRW
      build-depends:    base
                      , freer
                      , tasty
                      , tasty-hunit
                      , tasty-quickcheck
                      , QuickCheck

      ghc-options: -Wall

    benchmark core
      default-language: Haskell2010
      type:             exitcode-stdio-1.0
      hs-source-dirs:   bench
      main-is:          Core.hs
      build-depends:    base
                      , freer
                      , criterion
                      , mtl
                      , free

      ghc-options: -Wall -O2

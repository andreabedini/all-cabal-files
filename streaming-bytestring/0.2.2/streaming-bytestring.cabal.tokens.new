        , bytestring
        , deepseq
        , exceptions
        , ghc-prim           >=0.4     && <0.9
        , mmorph             >=1.0     && <1.3
        , mtl                >=2.1     && <2.3
        , resourcet
        , streaming          >=0.1.4.0 && <0.3
        , transformers       >=0.3     && <0.6
        , transformers-base

      if impl(ghc <7.8)
